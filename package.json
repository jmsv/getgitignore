{
  "name": "gitignore-dl",
  "version": "0.2.0",
  "description": "CLI tool to get a .gitignore file for your language from GitHub's collection",
  "main": "dist/gitignore-dl.js",
  "module": "dist/gitignore-dl.mjs",
  "typings": "dist/gitignore-dl.d.ts",
  "files": [
    "dist/*"
  ],
  "bin": "./bin.js",
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jmsv/gitignore-dl.git"
  },
  "keywords": [
    "gitignore",
    "github",
    "node",
    "typescript",
    "cli"
  ],
  "author": "James Vickery",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmsv/gitignore-dl/issues"
  },
  "homepage": "https://github.com/jmsv/gitignore-dl#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^18.0.0",
    "esbuild": "^0.14.48",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "commander": "^9.3.0",
    "inquirer": "^8.2.1"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  }
}
